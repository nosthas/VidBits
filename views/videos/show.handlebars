<h1>{{video.title}}</h1>
<div id="videos-container" class="contents-container">
   <div class="video-card">
    <div class="video-player">
        <iframe class="video-player" src="{{video.videoUrl}}" frameborder="0" allowfullscreen></iframe>
    </div>
    <div class="video-title">
      {{video.description}}
    </div>
  </div>
</div>
<a class="add-video-button" href="/video/add">Add Video</a>
